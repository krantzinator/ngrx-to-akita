{
  "name": "ngrx-to-akita",
  "version": "0.0.0",
  "description": "ngrx example app refitted with akita",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "start:auth": "node api/server/index.js",
    "build": "ng build",
    "lint": "ng lint && sass-lint -v -q",
    "test": "ng test",
    "e2e": "ng e2e",
    "postinstall": "ng build --prod"
  },
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.3.0",
    "yarn": ">=1.3.0 <2.0.0"
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "akita",
    "angular"
  ],
  "author": "Rob Wormald <robwormald@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ngrx/platform.git"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec",
      "**/spec/**/*",
      "/modules/schematics/src/*/files/**/*",
      "/modules/**/schematics/**/files/**/*",
      "**/schematics/src/utility/*"
    ],
    "include": [
      "**/*.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/bazel": "^6.1.0",
    "@angular/cdk": "^6.4.2",
    "@angular/cli": "^6.1.2",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.2",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/platform-server": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@datorama/akita": "^1.9.6",
    "@ngrx/db": "^2.1.0",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "ngrx-store-freeze": "^0.2.2",
    "ngx-take-until-destroy": "^5.1.0",
    "opencollective": "^1.0.3",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@ngrx/store": "^6.1.0",
    "@types/fs-extra": "^2.1.0",
    "@types/glob": "^5.0.33",
    "@types/globby": "^8.0.0",
    "@types/jasmine": "2.5.45",
    "@types/jasminewd2": "^2.0.2",
    "@types/jest": "^20.0.2",
    "@types/lodash": "^4.14.80",
    "@types/ncp": "^2.0.1",
    "@types/node": "^7.0.5",
    "@types/ora": "^1.3.1",
    "@types/rimraf": "^0.0.28",
    "akita-schematics": "^1.0.0-rc.4",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "codelyzer": "^4.2.0",
    "conventional-changelog": "^1.1.4",
    "conventional-changelog-cli": "^1.3.21",
    "coveralls": "^2.13.0",
    "cpy-cli": "^1.0.1",
    "deep-freeze": "^0.0.1",
    "fs-extra": "^2.1.2",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "jasmine": "^2.5.3",
    "jasmine-core": "~2.5.2",
    "jasmine-marbles": "^0.3.1",
    "jasmine-spec-reporter": "~3.2.0",
    "jest": "^21.0.2",
    "jest-preset-angular": "^3.0.1",
    "jest-zone-patch": "^0.0.7",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.2.2",
    "ncp": "^2.0.0",
    "nyc": "^10.1.2",
    "ora": "^1.3.0",
    "prettier": "^1.11.1",
    "protractor": "~5.1.0",
    "reflect-metadata": "^0.1.9",
    "replace-in-file": "^3.1.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.50.0",
    "sorcery": "^0.10.0",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.1.3",
    "tsickle": "^0.32.1",
    "tslib": "^1.7.1",
    "tslint": "^5.0.0",
    "tsutils": "2.20.0",
    "typescript": "~2.8.4",
    "uglify-js": "^3.1.9"
  }
}
